{
	"easycom": {
		"autoscan": true,
		"custom": {
			"^wd-(.*)": "wot-design-uni/components/wd-$1/wd-$1.vue"
		}
	},
	"tabBar": {
		"color": "#aaa",
		"selectedColor": "#000",
		"borderStyle": "black",
		"backgroundColor": "#fff",
		"height": "35px",
		"fontSize": "16px",
		"position": "bottom",
		"blurEffect": "extralight",
		"list": [
			{
				"pagePath": "pages/words/words",
				"text": "words"
			},
			{
				"pagePath": "pages/sentences/sentences",
				"text": "sentences"
			},
			{
				"pagePath": "pages/articles/articles",
				"text": "articles"
			},
			{
				"pagePath": "pages/user/user",
				"text": "user"
			}
		]
	},
	"pages": [
		{
			"path": "pages/words/words",
			"style": {
				"navigationBarTitleText": "words"
			}
		},
		{
			"path": "pages/sentences/sentences",
			"style": {
				"navigationBarTitleText": "uni-app"
			}
		},
		{
			"path": "pages/articles/articles",
			"style": {
				"navigationBarTitleText": "uni-app"
			}
		},
		{
			"path": "pages/user/user",
			"style": {
				"navigationBarTitleText": "uni-app"
			}
		}
	],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8"
	},
	"uniIdRouter": {}
}